    "nx": "22.1.3",
    "prettier": "^2.6.2",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tslib": "^2.3.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.40.0",
    "webpack-cli": "^5.1.4"
  },
  "workspaces": [
    "packages/*",
    "apps/*",
    "libs/*"
  ],
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
