{
  "name": "@auth-tutorial/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev:all": "concurrently --names \"API,WEB\" -c \"bgBlue.bold,bgGreen.bold\" \"npx nx run api:serve\" \"npx nx run web:dev\"",
    "dev:clean": "npm run kill-ports && npm run clean-locks && npm run dev:all",
    "kill-ports": "npx kill-port 3000 3333",
    "clean-locks": "node -e \"const fs = require('fs'); const lockPath = 'apps/web/.next/dev/lock'; if (fs.existsSync(lockPath)) fs.unlinkSync(lockPath);\"",
    "lint:all": "npx nx run-many --target=lint --all",
    "test:all": "npx nx run-many --target=test --all",
    "test:clean": "npm run kill-ports && npm run clean-locks && npm run test:all",
    "e2e:all": "npx nx run-many --target=e2e --all",
    "e2e:clean": "npm run kill-ports && npm run clean-locks && npm run e2e:all",
    "health-check": "npm run lint:all && npm run test:all && npm run e2e:all",
    "health-check:clean": "npm run kill-ports && npm run clean-locks && npm run health-check",
    "db:up": "docker compose up -d",
    "db:down": "docker compose down",
    "db:logs": "docker compose logs -f postgres",
    "db:studio": "cd apps/api && npx prisma studio",
    "db:migrate": "cd apps/api && npx prisma migrate dev",
    "db:generate": "cd apps/api && npx prisma generate",
    "api:generate": "orval --config orval.config.ts",
    "prepare": "husky"
  },
  "private": true,
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@eslint/js": "^9.39.1",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.9",
    "@nx/devkit": "22.1.3",
    "@nx/eslint": "22.1.3",
    "@nx/eslint-plugin": "22.1.3",
    "@nx/jest": "^22.1.3",
    "@nx/js": "22.1.3",
    "@nx/nest": "^22.1.3",
    "@nx/next": "^22.1.3",
    "@nx/node": "22.1.3",
    "@nx/playwright": "22.1.3",
    "@nx/react": "^22.1.3",
    "@nx/web": "22.1.3",
    "@nx/webpack": "22.1.3",
    "@orval/core": "^7.17.0",
    "@orval/zod": "^7.17.0",
    "@playwright/test": "^1.57.0",
    "@swc-node/register": "~1.11.1",
    "@swc/cli": "~0.7.9",
    "@swc/core": "~1.15.3",
    "@swc/helpers": "~0.5.17",
    "@swc/jest": "^0.2.39",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcrypt": "^6.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "24.10.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/pg": "^8.15.6",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/supertest": "^6.0.3",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "execa": "^9.6.1",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.7",
    "msw": "^2.12.4",
    "nx": "22.1.3",
    "orval": "^7.13.2",
    "prettier": "^3.7.4",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.1",
    "webpack-cli": "^6.0.1",
    "whatwg-fetch": "^3.6.20"
  },
  "workspaces": [
    "packages/*",
    "apps/*",
    "libs/*"
  ],
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
